library(ToDcallR)
library(gt)
install.packages("gt")
ToDcall_yang_loose <- readRDS("data/ToDcall_yang_data_loose.rds")
gt::gt(ToDcall_yang_loose@ToD_candidates_filtered)
as.data.frame(ToDcall_yang_loose@ToD_candidates_filtered)
#| label: tbl-example
